    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dev",
            "request": "launch",
            "type": "dart",
        },
        {
            "name": "Minbar",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=api-url=https://api.askimam.minbar.kz/v1",
                "--dart-define=audio-url=https://minbar.kz/askimam/audio",
            ],
        },
        {
            "name": "Azan",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--dart-define=api-url=https://api.askimam.azan.kz/v1",
                "--dart-define=audio-url=https://azan.kz/askimam/audio",
            ],
        }
    ]
}
